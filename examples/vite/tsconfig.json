{
	"compilerOptions": {
		"strict": true,
		"jsx": "preserve",
		"target": "esnext",
		"module": "esnext",
		"sourceMap": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"types": ["vite-plugin-builded-force-exit"],
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"useDefineForClassFields": true
	},
	"exclude": ["node_modules", "dist"]
}
